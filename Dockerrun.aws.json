{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
      {
          "name": "client",
          "image": "chaudhary1993/muti-client",
          "hostname": "client",
          "essential": "false",
          "memory": 128
      },

      {
        "name": "server",
        "image": "chaudhary1993/muti-server",
        "hostname": "api",
        "essential": "false",
        "memory": 128
      },

      {
        "name": "worker",
        "image": "chaudhary1993/muti-worker",
        "hostname": "worker",
        "essential": "false",
        "memory": 128
      },

      {
        "name": "nginx",
        "image": "chaudhary1993/muti-nginx",
        "essential": "true",
        "portMappings":[
            {
                "hostPort": 80,
                "containerPort": 80
            }
        ],
        "links": ["clientt", "serverr"],
        "memory": 128
      }

  ]

}